{
  "name": "fwe-time-tracker",
  "version": "1.0.0",
  "description": "an application for tracking time of tasks",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose --coverage --forceExit --runInBand",
    "test:watch": "jest --watchAll --verbose --runInBand",
    "test:ci": "jest --verbose --coverage --ci --forceExit --detectOpenHandles --runInBand",
    "start": "tsc-watch --outDir ./dist --onSuccess 'node --inspect=0.0.0.0:9229 ./dist/src/index'",
    "build": "tsc -b .",
    "lint": "tslint --project .",
    "typeorm": "node ./node_modules/typeorm/cli.js",
    "fixtures": "npx fixtures ./src/fixtures/ --config ormconfig.js"
  },
  "author": "Marcel Hauck",
  "license": "MIT",
  "dependencies": {
    "@types/body-parser": "^1.19.0",
    "body-parser": "^1.19.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "mysql2": "^2.2.5",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^26.4.4",
    "tsc-watch": "^4.2.9",
    "typeorm": "^0.2.29",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/supertest": "^2.0.10",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "supertest": "^6.0.1",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-etc": "^1.13.7",
    "tslint-plugin-prettier": "^2.3.0",
    "typeorm-fixtures-cli": "^1.8.0"
  }
}
